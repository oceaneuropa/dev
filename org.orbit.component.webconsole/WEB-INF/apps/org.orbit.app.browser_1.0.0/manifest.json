{
	"id": "org.orbit.app.browser",
	"version": "1.0.0",
	"name": "Browser",
	"type": "tool",
	"description": "Browser",
	"bundles": [
		{"Bundle-SymbolicName": "org.orbit.app.browser.ui", "Bundle-Version": "1.0.0"}
	],
	"classpath": [
		"plugins/org.orbit.app.browser.ui_1.0.0.jar"
	],
	"WEB-INF": [
		{ "name": "icon_s", "type": "image", "value": "images/browser_01_48x48.png"},
		{ "name": "icon_m", "type": "image", "value": "images/browser_01_64x64.png"},
		{ "name": "icon_l", "type": "image", "value": "images/browser_01_128x128.png"},
		{ "name": "js1",    "type": "js",    "value": "js/browser_1.js"},
		{ "name": "js2",    "type": "js",    "value": "js/browser_2.js"}
	],
	"properties": [
		{ "name": "prop1", "value": "value1"},
		{ "name": "prop2", "value": "value2"}
	],
	"preferences": [
		{ "name": "timeout",      "value": "100",     "type": "integer", "edit": "text"},
		{ "name": "autoComplete", "value": "true",    "type": "boolean", "edit": "checkbox"},
		{ "name": "defaultTitle", "value": "Browser", "type": "string",  "edit": "text"}
	],
	"extensions": [
		{
			"typeId": "org.orbit.app.icon",
			"id": "org.orbit.app.browser.icon",
			"icon_s": "images/browser_01_48x48.png",
			"icon_m": "images/browser_01_64x64.png",
			"icon_l": "images/browser_01_128x128.png"
		}
	]
}
